<script type="text/html" template>
    <link rel="stylesheet" href="{{ layui.setter.base }}style/template.css?v={{ layui.admin.v }}-1" media="all">
</script>

<form class="layui-form" id="formData" action="" lay-filter="example">
    <div class="layui-form-item">
        <label class="layui-form-label">名称</label>
        <div class="layui-input-block ">
            <input type="text" id="name" name="Name" maxlength="20" lay-verify="required|specialCharacters" autocomplete="off" placeholder="" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item">
        <label class="layui-form-label ">备注</label>
        <div class="layui-input-block">
            <textarea placeholder="" id="remark" name="Remark" lay-verify="describelength" class="layui-textarea"></textarea>
        </div>
    </div>
    <div class="layui-form-item content-center">
        <button class="layui-btn" type="button" lay-submit lay-filter="templatetype-form-submit">保存</button>
    </div>
</form>