        <div class="layui-row">
            <div class="layui-col-xs12 layui-col-sm12 layui-col-md12">
                题目：{{ d.params.name }}
            </div>
            <div class="layui-col-xs12 layui-col-sm12 layui-col-md12">
                矩阵量表题预览功能开发中
            </div>
        </div>
    </form>
    {{# } }}
</script>
