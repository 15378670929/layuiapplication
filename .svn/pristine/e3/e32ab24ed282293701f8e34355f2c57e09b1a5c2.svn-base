<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>爱智游大脑课堂管理系统</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="layui/css/layui.css" media="all">
    <link rel="shortcut icon" href="../src/style/favicon.ico" />
    <script>
        /^http(s*):\/\//.test(location.href) || alert('请先部署到 localhost 下再访问');
    </script>
</head>
<body>
    <div id="IQuizoo_app"></div>
    <script src="layui/layui.js" type="text/javascript"></script>
    <script src="../src/lib/oidc-client.js" type="text/javascript"></script>
    <script>

        layui.config({
            base: '../src/', //指定项目路径，本地开发用 src，线上用 dist
            version: '1.2.1'
        }).use('index')
            .extend({
                treeGrid: '../lib/treeGrid',
                treeSelect: '../lib/treeSelect',
                //oidcconfig: '../lib/oidcconfig' //相对于上述 base 目录的子目录
            });

    </script>
</body>
</html>